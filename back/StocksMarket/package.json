{
  "name": "stock-market-api",
  "version": "1.0.0",
  "description": "Production-ready distributed stock market trading API using gRPC microservices",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "market": "node market-service/server.js",
    "price": "node price-service/server.js",
    "investor": "node investor-service/server.js",
    "analytics": "node analytics-service/server.js",
    "webhook": "node webhook-service/server.js",
    "start:all": "concurrently \"npm run price\" \"npm run investor\" \"npm run analytics\" \"npm run market\" \"npm run webhook\""
  },
  "keywords": [
    "grpc",
    "microservices",
    "stock-market",
    "distributed-systems"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "express": "^4.18.2",
    "axios": "^1.6.2",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

